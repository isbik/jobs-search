{
  "semi": true,
  "singleQuote": false,
  "trailingComma": "all",
  "printWidth": 90,
  "tabWidth": 2,
  "bracketSpacing": true,
  "arrowParens": "always",
  "bracketSameLine": false,
  "jsxSingleQuote": false,
  "endOfLine": "lf",
  "singleAttributePerLine": true,
  "plugins": ["prettier-plugin-tailwindcss"]
}
